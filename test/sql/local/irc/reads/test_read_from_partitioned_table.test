require-env ICEBERG_SERVER_AVAILABLE

require avro

require parquet

require iceberg

require httpfs

require ducklake

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
set enable_logging=true

statement ok
set logging_level='debug'

statement ok
CREATE SECRET (
    TYPE S3,
    KEY_ID 'admin',
    SECRET 'password',
    ENDPOINT '127.0.0.1:9000',
    URL_STYLE 'path',
    USE_SSL 0
);


statement ok
ATTACH '' AS my_datalake (
    TYPE ICEBERG,
    CLIENT_ID 'admin',
    CLIENT_SECRET 'password',
    ENDPOINT 'http://127.0.0.1:8181'
);


query I
SELECT count(*) FROM my_datalake.default.month_partitioned_tabled where ts < TIMESTAMP '2025-02-01';
----
5

query I
SELECT count(*) FROM my_datalake.default.month_partitioned_tabled where ts <= TIMESTAMP '2025-02-01';
----
6

query I
SELECT count(*) FROM my_datalake.default.month_partitioned_tabled where ts = TIMESTAMP '2025-02-01';
----
1

query I
SELECT count(*) FROM my_datalake.default.month_partitioned_tabled where ts > TIMESTAMP '2025-02-01';
----
9

query I
SELECT count(*) FROM my_datalake.default.month_partitioned_tabled where ts >= TIMESTAMP '2025-02-01';
----
10


